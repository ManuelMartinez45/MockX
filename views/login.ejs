<!DOCTYPE html>
<html lang="en">
<head>
   <%- include('../partials/head.ejs') %> 
   <title>Login</title>
</head>
<body>
    <%- include('../partials/nav.ejs') %>
    <div class="grid-container full newGrid">
        <form class='loginDiv' action='/login' method='POST'>
            <h1>Login</h1>
              <div class="medium-12 cell">
                 <label>
                     Email: 
                    <input type="email" placeholder="example@email.com" name='email'>
                 </label>
              </div>
              <div class="medium-12 cell">
                 <label>
                     Password
                    <input id='password' type="password" name="password">
                 </label>
              </div>
           <input type="submit" value="Login" id='submitBtn' class='submitBtn button'>
     </form>
  </div>
</body>
</html>